{
  "name": "iniettore-project",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "lerna exec --concurrency 1 -- rimraf -rf dist",
    "dev": "jest \"(.*)\\.spec\\.ts\" --watch",
    "lint": "eslint src/**/*.js",
    "test": "jest \"(.*)\\.spec\\.js\" --coverage --coverageReporters=text-lcov | coveralls",
    "build": "yarn run clean && yarn run build:all",
    "build:all": "rollup --config ./rollup.config.js",
    "prepublish": "yarn build"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.0",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "coveralls": "~3.1.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^26.6.3",
    "jest-extended": "~0.11.5",
    "jest-leak-detector": "~26.6.2",
    "lerna": "^3.22.1",
    "rimraf": "^3.0.2",
    "rollup": "~2.39.0",
    "rollup-plugin-terser": "~7.0.2",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.5",
    "weak-napi": "^2.0.2"
  },
  "dependencies": {}
}
